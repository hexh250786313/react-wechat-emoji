{
  "name": "react-wechat-emoji",
  "version": "1.0.5",
  "description": "A WeChat like Emoji component for React.js",
  "main": "lib/index.js",
  "typings": "./src/Emoji/index.d.ts",
  "repository": "https://github.com/hexh250786313/react-wechat-emoji.git",
  "author": "hexh <250786313@qq.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "wechat",
    "emoji"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "babel src/Emoji --out-dir lib && cp -r src/Emoji/assets lib"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "eslintIgnore": [
    "*.ts",
    "node_modules/**/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,css,less,sass,scss}": [
      "git add"
    ]
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "10.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "7.17.0",
    "husky": "4.3.7",
    "lint-staged": "10.5.3",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "^4.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
